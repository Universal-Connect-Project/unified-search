{
  "name": "sophtron-search",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "dev": "npx nodemon src/server/index.js",
    "test": "jest --no-coverage --passWithNoTests",
    "test:watch": "jest --no-coverage --watch",
    "test:coverage": "npm test -- --verbose --coverage --watchAll",
    "test:clear": "npm test -- --clearCache",
    "precommit": "lint-staged",
    "prepare": "husky"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.245.0",
    "@aws-sdk/lib-storage": "^3.245.0",
    "axios": "^0.28.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.19.2",
    "readline": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-react": "^7.24.1",
    "@types/jest": "^29.5.12",
    "cypress": "^13.7.3",
    "eslint": "^8.57.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-jest": "^28.2.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "msw": "^2.2.14",
    "nodemon": "^3.0.3",
    "ts-jest": "^29.1.2"
  },
  "lint-staged": {
    "{.eslintrc.js}": [
      "prettier --write"
    ],
    "*.{js[x],ts[x]}": [
      "npm run lint -- --cache --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "prettier --write . && lint-staged && npm run test && git add -A ."
    }
  }

}
